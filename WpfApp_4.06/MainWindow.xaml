<Window x:Class="WpfApp_4._06.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp_4._06"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Content="Launch Notepad" Click="LaunchNotepad_Click" Width="200" Margin="5"/>
            <Button Content="Launch Calculator" Click="LaunchCalculator_Click" Width="200" Margin="5"/>
            <Button Content="Launch Paint" Click="LaunchPaint_Click" Width="200" Margin="5"/>
            <Button Content="Launch Custom App" Click="LaunchCustomApp_Click" Width="200" Margin="5"/>
        </StackPanel>
    </Grid>
</Window>
